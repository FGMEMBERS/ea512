<animation>
  <type>material</type>
  <object-name>cabincomm</object-name>
<condition><property>/instrumentation/boardcomm/cabincomm</property>
</condition>
  <diffuse>
   <red>1</red>
   <green>1</green>
   <blue>1</blue>
  </diffuse>
  <ambient>
   <red>1</red>
   <green>1</green>
   <blue>1</blue>
  </ambient>
<emission>	
  <red>1</red>	
  <green>0</green>	
  <blue>0</blue>
 </emission>
 </animation>
<animation>
  <type>pick</type>
  <object-name>seatbeltsonoff</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>  
      <binding>
        <command>property-toggle</command>
        <property>/instrumentation/boardcomm/seatbelts</property>
      </binding>
    </action>
  </animation>
<animation>
  <type>rotate</type>
  <object-name>seatbeltsonoff</object-name>
        <property>/instrumentation/boardcomm/seatbelts</property>
<factor>-35</factor>
<center>
<x-m>0.00252</x-m>
<z-m>-0.00479</z-m>
</center>
<axis><y>1</y></axis>
  </animation>
<animation>
  <type>pick</type>
  <object-name>smokingonoff</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>  
      <binding>
        <command>property-toggle</command>
        <property>/instrumentation/boardcomm/nosmoking</property>
      </binding>
    </action>
  </animation>
<animation>
  <type>rotate</type>
  <object-name>smokingonoff</object-name>
        <property>/instrumentation/boardcomm/nosmoking</property>
<factor>-35</factor>
<center>
<x-m>0.00252</x-m>
<z-m>-0.00479</z-m>
</center>
<axis><y>1</y></axis>
  </animation>
<animation>
  <type>pick</type>
  <object-name>cabincall</object-name>
<visible>true</visible>
    <action>
      <button>0</button> 
      <binding>
        <command>property-toggle</command>
        <property>/instrumentation/boardcomm/cabincall</property>
      </binding>
<mod-up> 
      <binding>
        <command>property-toggle</command>
        <property>/instrumentation/boardcomm/cabincall</property>
      </binding>
</mod-up>
    </action>
  </animation>
<animation>
  <type>translate</type>
  <object-name>cabincall</object-name>
        <property>/instrumentation/boardcomm/cabincall</property>
<factor>-0.001</factor>
<axis><x>1</x></axis>
  </animation>
<animation>
  <type>pick</type>
  <object-name>cabincomm</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>  
      <binding>
        <command>property-toggle</command>
        <property>/instrumentation/boardcomm/cabincomm</property>
      </binding>
<mod-up> 
      <binding>
        <command>property-toggle</command>
        <property>/instrumentation/boardcomm/cabincomm</property>
      </binding>
</mod-up>
    </action>
  </animation>
<animation>
  <type>translate</type>
  <object-name>cabincomm</object-name>
        <property>/instrumentation/boardcomm/cabincomm</property>
<factor>-0.001</factor>
<axis><x>1</x></axis>
  </animation>
